<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="applicationConfiguration" class="pl.madshai.rjmock.configuration.ApplicationConfiguration"/>

    <bean id="mocksConfigurationReader" class="pl.madshai.rjmock.configuration.MocksConfigurationReader"/>

    <bean id="mocksReader" class="pl.madshai.rjmock.mocks.MockReader"/>

    <bean id="mocksResponseReader" class="pl.madshai.rjmock.mocks.MocksResponseReader"/>
    
    <bean id="mocksConfiguration" class="pl.madshai.rjmock.configuration.MocksConfiguration" />

</beans>
